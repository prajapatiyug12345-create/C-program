#include <stdio.h>

int main() {
    // 3x3 array
    int matrix[3][3] = {
        {11, 12, 13},
        {21, 22, 23},
        {31, 32, 33}
    };
    int N = 3; 

    printf("17. Diagonal elements:\n");
    for (int i = 0; i < N; i++) {

        int *diag_ptr = &matrix[i][i]; 
        
        printf("Element at [%d][%d]: %d\n", i, i, *diag_ptr);
    }
    
    return 0;
}
