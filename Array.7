#include <stdio.h>

int main() {
    int R = 4, C = 4; // 4x4 matrix
    int Original[4][4];
    int Transpose[4][4];
    int i, j;
  
    int count = 1;
    for (i = 0; i < R; i++) {
        for (j = 0; j < C; j++) {
            Original[i][j] = count++;
        }
    }
    for (i = 0; i < R; i++) {
        for (j = 0; j < C; j++) {
            // Swap (i, j) with (j, i)
            Transpose[j][i] = Original[i][j]; 
        }
    }
    printf("Original Matrix:\n");
    for (i = 0; i < R; i++) {
        for (j = 0; j < C; j++) {
            printf("%d\t", Original[i][j]);
        }
        printf("\n");
    }
    printf("\nTransposed Matrix:\n");
    for (i = 0; i < R; i++) {
        for (j = 0; j < C; j++) {
            printf("%d\t", Transpose[i][j]);
        }
        printf("\n");
    }

    return 0;
}
