#include <stdio.h>

int main() {
    int R1 = 3, C1 = 3; // A is 3x3
    int R2 = 3, C2 = 3; // B is 3x3
    int A[3][3], B[3][3], C[3][3];
    int i, j, k;
    printf("Enter elements for Matrix A (3x3) and B (3x3).\n");
    
    for (i = 0; i < R1; i++) {
        for (j = 0; j < C1; j++) {
            A[i][j] = 1; 
            B[i][j] = 2; 
            C[i][j] = 0; 
        }
    }
    for (i = 0; i < R1; i++) { 
        for (j = 0; j < C2; j++) { 
            for (k = 0; k < C1; k++) { 
                C[i][j] += A[i][k] * B[k][j];
            }
        }
    }
    printf("\nResultant Matrix C (A x B):\n");
    for (i = 0; i < R1; i++) {
        for (j = 0; j < C2; j++) {
            printf("%d\t", C[i][j]);        
        printf("\n");
    }

    return 0;
}
