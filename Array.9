#include <stdio.h>

int main() {
    int size = 5;
    int arr[5];
    int i, temp;
    printf("Enter 5 integer values:\n");
    for (i = 0; i < size; i++) {
        printf("Enter value %d: ", i + 1);
        scanf("%d", &arr[i]);
    }
    for (i = 0; i < size / 2; i++) {
        temp = arr[i];
        arr[i] = arr[size - 1 - i];
        arr[size - 1 - i] = temp;
    }
    printf("\nReversed Array:\n");
    for (i = 0; i < size; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");

    return 0;
}
