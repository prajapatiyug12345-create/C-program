#include <stdio.h>

int main() {
    int size = 10;
    // Example array for testing:
    int arr[10] = {5, 2, 8, 2, 5, 8, 5, 1, 8, 8};
    // visited array tracks elements that have already been counted
    int visited[10]; 
    int i, j, count; 
    for (i = 0; i < size; i++) {
        visited[i] = -1;
    }

    printf("Input Array: 5 2 8 2 5 8 5 1 8 8\n");
    printf("\nFrequency of each element:\n");
    for (i = 0; i < size; i++) {
        if (visited[i] != 0) {
            count = 1;
            
            for (j = i + 1; j < size; j++) {
                if (arr[i] == arr[j]) {
                    count++;
                    
                    visited[j] = 0; 
                }
            }
            
            printf("%d occurs %d time(s)\n", arr[i], count);
            visited[i] = 0; 
        }
    }

    return 0;
}
