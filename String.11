#include <stdio.h>

int main() {
    char str[100];
    int i = 0, length;
    
    printf("Enter a string: ");
    scanf("%[^\n]s", str); 
    while (str[i] != '\0') {
        i++;
    }
    length = i;
    
    printf("Reversed string printed vertically:\n");
    for (i = length - 1; i >= 0; i--) {
        printf("%c\n", str[i]);
    }
    
    return 0;
}
