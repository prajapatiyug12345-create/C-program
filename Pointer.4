#include <stdio.h>

int main() {
    int a = 10;
    int b = 20;

    int *ptr_a = &a;
    int *ptr_b = &b;
    
    int temp;

    printf("Before swap: a = %d, b = %d\n", a, b);
    temp = *ptr_a; 
    
    *ptr_a = *ptr_b;
    
    *ptr_b = temp;

    printf("After swap: a = %d, b = %d\n", a, b);
    
    return 0;
}
