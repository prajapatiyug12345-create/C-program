#include <stdio.h>

int main() {
    int R = 3, C = 3; // 3x3 array size
    int A[3][3], B[3][3], C[3][3];
    int i, j;

    // --- Input Array A ---
    printf("Enter elements for Matrix A (%dx%d):\n", R, C);
    for (i = 0; i < R; i++) {
        for (j = 0; j < C; j++) {
            printf("A[%d][%d]: ", i, j);
            scanf("%d", &A[i][j]);
        }
    }
    printf("Enter elements for Matrix B (%dx%d):\n", R, C);
    for (i = 0; i < R; i++) {
        for (j = 0; j < C; j++) {
            printf("B[%d][%d]: ", i, j);
            scanf("%d", &B[i][j]);
        }
    }
    for (i = 0; i < R; i++) {
        for (j = 0; j < C; j++) {
            C[i][j] = A[i][j] + B[i][j];
        }
    }
    printf("\nResultant Matrix C (A + B):\n");
    for (i = 0; i < R; i++) {
        for (j = 0; j < C; j++) {
            printf("%d\t", C[i][j]);
        }
        printf("\n");
    }

    return 0;
}
